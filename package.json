{
  "name": "app-labs-v3",
  "version": "3.2.2",
  "scripts": {
    "css-build": "sass static/css/main.scss static/css/main.css",
    "package": "svelte-kit package",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "preview-host": "vite preview --host",
    "clean-docs": "rmdir /S /Q docs && mkdir docs",
    "copy-export": "cp -a -r build/* docs/",
    "commit-export": "git add docs && git commit -m ",
    "update-docs": "npm run clean-docs && npm run build && npm run copy-export && touch docs/.nojekyll && npm run commit-export"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0",
    "@sveltejs/kit": "^1.7.1",
    "bulma": "^0.9.4",
    "sass": "^1.54.4",
    "svelte": "^3.54.0",
    "vite": "4.0.0"
  },
  "type": "module"
}
