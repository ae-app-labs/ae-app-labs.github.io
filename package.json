{
  "name": "app-labs-v3",
  "version": "3.0.9",
  "scripts": {
    "dev": "svelte-kit dev",
    "dev-host": "svelte-kit dev --host",
    "css-build": "node-sass --omit-source-map-url public/css/main.scss public/css/main.css",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "clean-docs": "rmdir /S /Q docs && mkdir docs",
    "copy-nojekyll": "cp -a .nojekyll docs/",
    "copy-export": "cp -a -r build/* docs/",
    "commit-export": "git add docs && git commit -m ",
    "update-docs": "npm run clean-docs && npm run build && npm run copy-export && npm run copy-nojekyll && npm run commit-export"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "^1.0.0-next.34",
    "@sveltejs/kit": "next",
    "bulma": "^0.9.4",
    "node-sass": "^7.0.1",
    "svelte": "^3.48.0"
  },
  "type": "module"
}
